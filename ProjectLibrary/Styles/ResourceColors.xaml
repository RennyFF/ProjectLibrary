<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Background Colors  -->
    <SolidColorBrush x:Key="MainBackgroundColorBrush" Color="#FF0E0E0E" />
    <SolidColorBrush x:Key="SecondBackgroundColorBrush" Color="#FF131313" />
    <SolidColorBrush x:Key="HighLightBackgroundColorBrush" Color="#FF252525" />
    <SolidColorBrush x:Key="HoverBackgroundColorBrush" Color="#FF505050" />
    <SolidColorBrush x:Key="ImportantRedBrush" Color="#FFF34646" />

    <Color x:Key="MainBackgroundColor">#FF0E0E0E</Color>
    <Color x:Key="SecondBackgroundColor">#FF131313</Color>
    <Color x:Key="HighLightBackgroundColor">#FF252525</Color>
    <Color x:Key="HoverBackgroundColor">#FF505050</Color>
    <Color x:Key="ImportantRed">#FFF34646</Color>

    <!--  Text Colors  -->
    <SolidColorBrush x:Key="MainTextColor" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="SecondTextColor" Color="#FF505050" />


    <Color x:Key="MainColor">#FFFFFFFF</Color>
    <Color x:Key="SecondColor">#FF505050</Color>
    <Color x:Key="ThirdColor">#FF0E0E0E</Color>

</ResourceDictionary>